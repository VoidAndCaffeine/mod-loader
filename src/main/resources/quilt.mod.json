{
	"schema_version": 1,
	"quilt_loader": {
		"group": "com.example",
		"id": "voids_mod_loader",
		"version": "${version}",
		"metadata": {
			"name": "Void's Mod Loader",
			"description": "a hopefully simple (tm) utility to download and update mods for BBSMP",
			"license":"MIT"
			"contributors": {
				"Void & Caffeine": "Owner"
			},
			"contact": {
				"homepage": "https://github.com/VoidAndCaffeine/mod-loader",
				"issues": "https://github.com/VoidAndCaffeine/mod-loader/issues",
				"sources": "https://github.com/VoidAndCaffeine/mod-loader"
			},
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": "io.github.VoidAndCaffeine.voids_mod_loader.VoidsModLoader"
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.18.10"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=7.0.0"
			},
			{
				"id": "minecraft",
				"versions": ">=1.20"
			}
		]
	},
	"mixin": "example_mod.mixins.json"
